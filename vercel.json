{
  "version": 2,
  "name": "demo-bot",
  "builds": [
    {
      "src": "src/main.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/main.ts",
      "methods": [
          "GET",
          "POST",
          "PUT",
          "DELETE"
      ]
  }
  ]
}